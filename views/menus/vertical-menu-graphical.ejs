<div class="vertical-menu-wrapper">
    <% for(var i = 0; i < menuItems.length; i += 1) { %>
        <a class="bg_image" 
            href="<%= menuItems[i]['urlString'] %>" 
            title="<%= menuItems[i]['help_text'] %>"
        >
            <%= menuItems[i]["title"] %>
        </a>
    <% } %>
</div>